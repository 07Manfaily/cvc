  function returnColumn() {

        return (

            column.length > 0 ? column.map((col) => {
                <ListItem key={col.key} component="div" disablePadding>

                    <ListItemText primary={"col.to_display"} />

                </ListItem>
            })
                : "");



    }
<Accordion>
                        <AccordionSummary

                            aria-controls="panel3-content"
                            id="panel3-header"
                        >
                            <FilterListIcon />
                        </AccordionSummary>
                        <AccordionDetails>
                            <FixedSizeList
                                height={400}
                                width={360}
                                itemSize={46}
                                itemCount={column.length}
                                overscanCount={5}
                            >
                                {returnColumn}
                            </FixedSizeList>
                        </AccordionDetails>

                    </Accordion>
